{
  "PostgreSQL": {
    "ConnectionString": "Host=culinarydb;Port=5432;Database=CulinarDB;Username=Culinar334;Password=culinarypasswd"
  },
  "RabbitMQ": {
    "HostName": "rabbitmq"
  },
  "MinioLocalHost": "http://localhost:9000",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.AspNetCore.Hosting": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/culinar-.log",
          "retainedFileCountLimit": 2,
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "Minio": {
    "Host": "localhost:9000",
    "Proxy": "minio",
    "ProxyPort": 9000,
    "AccessKey": "culinaryadmin",
    "SecretKey": "culinarypassword"
  },
  "AllowedHosts": "*"
}
